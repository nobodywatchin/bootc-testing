[highavailability]
name=AlmaLinux Kitten $releasever - HighAvailability
mirrorlist=https://kitten.mirrors.almalinux.org/mirrorlist/$releasever-kitten/highavailability
# baseurl=https://kitten.repo.almalinux.org/$releasever-kitten/HighAvailability/$basearch/os/
enabled=0
gpgcheck=1
countme=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-AlmaLinux-10
metadata_expire=86400
enabled_metadata=1

[highavailability-debuginfo]
name=AlmaLinux Kitten $releasever - HighAvailability - Debug
